 {
  "manifest_version": 2,
  "name": "Subadub",
  "description" : "Enhanced Netflix subtitles for foreign language study. Now works on twoseven.xyz",
  "version": "0.1.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*", "https://twoseven.xyz/*"],
      "all_frames": true,
      "run_at": "document_end",
      "js": ["content_script.js"]
    }
  ]
 }
